\begin{tikzpicture}

\pgfplotsset{
    small,
	width=\figurewidth,
	height=\figureheight,
	scale only axis,
    every tick/.style={color=black, thin},
    every axis plot/.append style={thick}
}

\begin{axis}[%
    axis y line* = box,
    axis x line* = box,
    legend style={
        font=\small,
        legend pos=north west,
        draw=none,
        fill=none,
        legend cell align=left},
    % error bars/error bar style={thick},
    error bars/error mark={triangle*},
