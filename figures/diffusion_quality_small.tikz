    xmax = 240,
    xminorticks = true,
    extra x ticks = {1, 6.7, 36, 240},
    extra x tick labels = { , 6.7, 36, 240},
    extra tick style={grid=major},
    xlabel = {$\seedDensQuot$},
    xlabel near ticks,
    xmode = log,
    enlargelimits={abs=3mm},
    ylabel={$E^{V, f}_\text{diff}$},
    yticklabel style = {/pgf/number format/fixed}
]

% Plots for TEMP

\addplot[
color=mycolor1,
forget plot,
error bars/.cd,
y dir = minus,
y explicit
]
